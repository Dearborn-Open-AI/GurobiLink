Paclet[
	"Name" -> "${PROJECT_NAME}",
	"Version" -> "${PACLET_VERSION}",
	"WolframVersion" -> "${TARGET_WOLFRAM_VERSION}",
	"AutoUpdating" -> True,
	"SystemID" -> "${SYSTEMID}",
	"Qualifier" -> "${QUALIFIER}",
	"Extensions" ->
		{
			{
				"Kernel",
					"Context" -> "${PROJECT_NAME}`"
			}
			,
			{
				"LibraryLink",
					"Root" -> "LibraryResources"
			}
		}
]
